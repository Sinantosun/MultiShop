@model ResultFeatureByIdDto
@{
    ViewData["Title"] = "UpdateFeature";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}


<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="chart-box">
                <h4>Yeni Öne Çıkan Ekleme Sayfası </h4>
                <form method="post">
                    <input type="hidden" asp-for="Id" >
                    <div class="form-group">
                        <label for="exampleInputForIcon">İkon</label>
                        <input type="text" asp-for="Icon" class="form-control" id="exampleInputForIcon" placeholder="Lütfen ikon giriniz">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputForTitle">Başlık</label>
                        <input type="text" asp-for="Title" class="form-control" id="exampleInputForTitle" placeholder="Lütfen başlık yazınız">
                    </div>
                    <button type="submit" class="btn btn-sm btn-primary">Güncelle</button>
                </form>
            </div>
        </div>
    </div>
</section>

